  title = {Carlo {{Rovelli}} | {{Helgoland}}: {{Making Sense}} of the {{Quantum Revolution}} | {{Talks}} at {{Google}}},
  shorttitle = {Carlo {{Rovelli}} | {{Helgoland}}},
