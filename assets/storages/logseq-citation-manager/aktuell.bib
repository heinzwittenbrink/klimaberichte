@article{universityofpotsdamAtlanticOverturningCirculation2024,
  title = {Is the {{Atlantic Overturning Circulation Approaching}} a {{Tipping Point}}?},
  author = {{University of Potsdam} and Rahmstorf, Stefan},
  date = {2024},
  journaltitle = {Oceanography},
  shortjournal = {Oceanog},
  issn = {10428275},
  doi = {10.5670/oceanog.2024.501},
  url = {https://tos.org/oceanography/article/is-the-atlantic-overturning-circulation-approaching-a-tipping-point},
  urldate = {2024-05-22},
  abstract = {The Atlantic Meridional Overturning Circulation has a major impact on climate, not just in the northern Atlantic but globally. Paleoclimatic data show it has been unstable in the past, leading to some of the most dramatic and abrupt climate shifts known. These instabilities are due to two different types of tipping points, one linked to amplifying feedbacks in the large-scale salt transport and the other in the convective mixing that drives the flow. These tipping points present a major risk of abrupt ocean circulation and climate shifts as we push our planet further out of the stable Holocene climate into uncharted waters.}
}

